{
  "name": "nodejs-es6-template",
  "version": "1.0.0",
  "description": "Node.js es6 starter template",
  "main": "src/index.js",
  "scripts": {
    "build": "rimraf dist/ && babel ./src/ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/index.js"
  },
  "keywords": [
    "nodejs",
    "jsvascript",
    "es6",
    "nodemon",
    "babel",
    "docker"
  ],
  "author": "gcoderbh",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
